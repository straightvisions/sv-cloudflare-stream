{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "straightvisions/sv-cloudflare-stream",
	"title": "SV Cloudflare Stream",
	"icon": "video-alt3",
	"category": "media",
	"description": "Embed a video from your media library or upload a new one.",
	"keywords": [ "video", "movie", "stream" ],
	"textdomain": "sv-cloudflare-stream",
	"editorScript": "sv-cloudflare-stream-editor-script",
	"editorStyle": "sv-cloudflare-stream-editor-style",
	"style": "sv-cloudflare-stream-style",
	"attributes": {
		"autoplay": {
			"type": "boolean"
		},
		"controls": {
			"type": "boolean"
		},
		"uid": {
			"type": "string"
		},
		"loop": {
			"type": "boolean"
		},
		"muted": {
			"type": "boolean"
		},
		"poster": {
			"type": "string"
		},
		"src": {
			"type": "string"
		},
		"playsInline": {
			"type": "boolean"
		},
		"tracks": {
			"type": "array",
			"items": {
				"type": "object"
			},
			"default": []
		}
	},
	"supports": {
		"anchor": true,
		"align": [ "full", "wide" ]
	}
}